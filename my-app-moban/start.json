{
  "apps": [
    {
      "name": "algorithms",
      "script": "npm start",
      "cwd": "./",
      "watch": [
        "pages",
        "style",
        "util",
        "common",
        "components",
        "static"
      ],
      "ignore_watch": [
        ".next",
        "build",
        "node_modules"
      ],
      "watch_options": {
        "followSymlinks": false
      },
      "env": {
        "NODE_ENV": "production"
      },
      "env_dev": {
        "NODE_ENV": "development"
      },
      "env_test": {
        "NODE_ENV": "test"
      }
    }
  ]
}
